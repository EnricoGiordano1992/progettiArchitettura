.model SEGNALE
.inputs R S7 S6 S5 S4 S3 S2 S1 S0
.outputs OUT1 OUT0

.subckt ZERO \
	ZERO1=CONST0

.subckt UGUALE_ZERO \
	I1=R I0=CONST0 \
	O0=R_OUT

.subckt VELOCITA \
	I7=S7 I6=S6 I5=S5 I4=S4 I3=S3 I2=S2 I1=S1 I0=S0 \
	O0=OUT0

.subckt AND \
	I1=R_OUT I0=S_OUT \
	O0=OUT1

.search and.blif
.search uguale_zero.blif
.search velocita.blif
.search zero.blif

.end

